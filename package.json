  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build --env=prod && tsc --project server",
    "start": "pm2 start dist/www.js --no-daemon",
    "build-start": "npm run build && npm start",
    "dev": "parallelshell \"ng serve --proxy-config proxy.conf.json\" \"npm run api\" ",
    "prod": "NODE_ENV=production npm run build-start",
    "api": "nodemon --watch server --ext ts --ignore *.spec.ts --exec ts-node --project server server/www ",
    "lint": "tslint \"client/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
